{
  "compilerOptions": {
    "declaration": true,
    "target": "es5",
    "module": "commonjs",
    "outDir": "dist",
    "strict": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "es5",
      "dom",
      "es2015.promise",
      "es2015",
      "es2017"
    ],
    "types": [
      "cypress"
    ]
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
